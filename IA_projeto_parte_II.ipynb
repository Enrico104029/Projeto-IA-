        "import json\n",
        "\n",
        "def parse_cast(cast_str):\n",
        "  try:\n",
        "    cast_list = json.loads(cast_str)\n",
        "    return [cast_member['name'] for cast_member in cast_list]\n",
        "  except:\n",
        "    return []\n",
        "\n",
        "df_credits['cast'] = df_credits['cast'].apply(parse_cast)\n",
        "display(df_credits.head())"
      ]
