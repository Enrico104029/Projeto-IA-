        "            if cast_member not in all_cast:\n",
        "                all_cast.append(cast_member)\n",
        "    elif isinstance(cast_members, str):\n",
        "        for cast_member in cast_members.split('|'):\n",
        "            if cast_member not in all_cast:\n",
        "                all_cast.append(cast_member)\n",
        "\n"
