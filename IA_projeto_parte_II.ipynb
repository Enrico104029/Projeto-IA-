      "source": [
        "def create_binary_list(items, unique_items_list):\n",
        "    binary_list = []\n",
        "    if isinstance(items, list):\n",
        "        for item in unique_items_list:\n",
        "            if item in items:\n",
        "                binary_list.append(1)\n",
        "            else:\n",
        "                binary_list.append(0)\n",
        "    elif isinstance(items, str):\n",
        "        item_list = items.split('|')\n",
        "        for item in unique_items_list:\n",
        "            if item in item_list:\n",
        "                binary_list.append(1)\n",
        "            else:\n",
        "                binary_list.append(0)\n",
        "    else:\n",
        "        binary_list = [0] * len(unique_items_list)\n",
        "\n",
        "    return binary_list\n",
        "\n",
        "df_merged['genres_binary'] = df_merged['genres'].apply(lambda x: create_binary_list(x, all_genres))\n",
        "df_merged['cast_binary'] = df_merged['cast'].apply(lambda x: create_binary_list(x, all_cast))\n",
        "\n",
        "display(df_merged[['genres', 'genres_binary', 'cast', 'cast_binary']].head())"
