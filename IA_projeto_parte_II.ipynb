        "import json\n",
        "\n",
        "def parse_genres(genres_str):\n",
        "  try:\n",
        "    genres_list = json.loads(genres_str)\n",
        "    return [genre['name'] for genre in genres_list]\n",
        "  except:\n",
        "    return []\n",
        "\n",
        "df_movies['genres'] = df_movies['genres'].apply(parse_genres)\n",
        "display(df_movies.head())"
